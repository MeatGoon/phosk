<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.teamproject.phosk.option.dao.AddoptionDAO">

	
	<insert id="insertaddoption" >
		insert into phosk_basicOption(branch_num,category_num,item_num,add_option,add_price)
		values(#{branch_num},#{category_num},#{item_num},#{add_option},#{add_price})	
	</insert>
	
	
</mapper>